<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
	<struts>
	    <package name="app" extends="struts-default" namespace="/apphybrid">

			<action name="getproductlist" class="com.smart4c.action.apphybrid.AppHybridAction" method="getProductList">
				<result name="success">/apphybrid/product.jsp</result>
			</action>

			<action name="my" class="com.smart4c.action.apphybrid.AppHybridAction" method="getOrderByState">
				<result name="success">/apphybrid/my.jsp</result>
			</action>

			<action name="loginByAccessToken" class="com.smart4c.action.apphybrid.AppHybridAction" method="loginByAccessToken">

			</action>

			<action name="bindByAccessToken" class="com.smart4c.action.apphybrid.AppHybridAction" method="bindByAccessToken">

			</action>

			<action name="my-order" class="com.smart4c.action.apphybrid.AppHybridAction" method="getAllOrderByState">
				<result name="success">/apphybrid/my-order.jsp</result>
			</action>

			<action name="orderComfirm" class="com.smart4c.action.apphybrid.AppHybridAction" method="createOrder">
				<result name="success">/apphybrid/order-confirm2.jsp</result>
			</action>


			<action name="cancerOrder" class="com.smart4c.action.apphybrid.AppHybridAction" method="cancerOrder">
			</action>

			<action name="sureOrder" class="com.smart4c.action.apphybrid.AppHybridAction" method="sureOrder">
			</action>

			<action name="sureComment" class="com.smart4c.action.apphybrid.AppHybridAction" method="sureComment">
			</action>

			<action name="refundOrder" class="com.smart4c.action.apphybrid.AppHybridAction" method="refundOrder">
			</action>

			<action name="storeChoose" class="com.smart4c.action.apphybrid.AppHybridAction" method="getShopList">
				<result name="success">/apphybrid/store-choose.jsp</result>
			</action>

			<action name="trade" class="com.smart4c.action.apphybrid.AppHybridAction" method="tradePay">
			</action>

			<action name="callBack" class="com.smart4c.action.apphybrid.AppHybridAction" method="callBack">
			</action>

			<action name="queryServiceShopByPhone" class="com.smart4c.action.apphybrid.AppHybridAction" method="queryServiceShopByPhone">
			</action>

	    </package>
    </struts>